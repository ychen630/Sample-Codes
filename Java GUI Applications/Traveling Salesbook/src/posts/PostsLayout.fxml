<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="postViewMain" prefHeight="573.0" prefWidth="557.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="posts.PostLayoutController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" onMouseClicked="#cancelListing" text="Back to main page">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </Label>
      <Button layoutX="413.0" layoutY="516.0" mnemonicParsing="false" onMouseClicked="#editPost" text="Edit">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button layoutX="465.0" layoutY="516.0" mnemonicParsing="false" onMouseClicked="#removePost" text="Remove">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button layoutX="359.0" layoutY="516.0" mnemonicParsing="false" onMouseClicked="#addPost" text="Add">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Label layoutX="20.0" layoutY="48.0" text="Posts">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
<!--       <ListView fx:id="postList" layoutX="19.0" layoutY="82.0" prefHeight="422.0" prefWidth="520.0" /> -->
   </children>
</AnchorPane>
