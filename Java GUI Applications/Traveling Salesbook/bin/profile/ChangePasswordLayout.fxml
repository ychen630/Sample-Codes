<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="changePasswordMain" prefHeight="240.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="profile.ChangePasswordController">
   <children>
      <GridPane layoutX="25.0" layoutY="6.0" prefHeight="177.0" prefWidth="200.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <PasswordField fx:id="currentPassword"  prefWidth="200.0" promptText="Current Password" GridPane.rowIndex="1">
               <font>
                  <Font size="14.0" />
               </font>
            </PasswordField>
            <PasswordField fx:id="newPassword" prefWidth="200.0" promptText="New Password" GridPane.rowIndex="2">
               <font>
                  <Font size="14.0" />
               </font>
            </PasswordField>
            <PasswordField fx:id="confirmNewPassword" prefWidth="200.0" promptText="Confirm New Password" GridPane.rowIndex="3">
               <font>
                  <Font size="14.0" />
               </font>
            </PasswordField>
            <Label text="Change Password">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </GridPane>
      <Button layoutX="83.0" layoutY="193.0" mnemonicParsing="false" onMouseClicked="#changePassword" text="Change">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="164.0" layoutY="193.0" mnemonicParsing="false" onMouseClicked="#cancel" text="Cancel">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
